{
  "editor.formatOnSave": true,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dart]": {
    "editor.defaultFormatter": "Dart-Code.dart-code"
  },
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "editor.tabSize": 2,
  "cSpell.words": [
    "codetable",
    "grayscale",
    "greyscale",
    "instanse",
    "qrcode",
    "rasterization",
    "resterized",
    "rgba",
    "sublist"
  ]
}
